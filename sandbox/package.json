{
  "name": "marble-schematics-sandbox",
  "version": "0.0.0",
  "description": "Sandbox app for Marble.js schematics",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "watch": "nodemon --watch src --ext ts --ignore '*.spec.ts' --exec yarn start",
    "docker": "docker-compose -f .docker/docker-compose.yml up --build -d"
  },
  "private": true,
  "engines": {
    "node": ">= 8.0.0",
    "yarn": ">= 1.7.0",
    "npm": ">= 5.0.0"
  },
  "dependencies": {
    "@marblejs/core": "2.3.1",
    "@marblejs/middleware-body": "2.3.1",
    "@marblejs/middleware-cors": "2.3.1",
    "@marblejs/middleware-io": "2.3.1",
    "@marblejs/middleware-jwt": "2.3.1",
    "@marblejs/middleware-logger": "2.3.1",
    "@types/faker": "4.1.9",
    "@types/joi": "14.3.4",
    "@types/mongoose": "5.5.41",
    "@types/node": "13.1.8",
    "@types/uuid": "7.0.3",
    "chalk": "3.0.0",
    "faker": "4.1.0",
    "mongoose": "5.8.9",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "tsconfig-paths": "3.9.0",
    "typegoose": "5.9.1",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "nodemon": "2.0.2",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  }
}
