{
  "name": "marble-schematics-sandbox",
  "version": "0.0.0",
  "description": "Sandbox app for Marble.js schematics",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "watch": "nodemon --watch src --ext ts --ignore '*.spec.ts' --exec yarn start",
    "docker": "docker-compose -f .docker/docker-compose.yml up --build -d"
  },
  "private": true,
  "engines": {
    "node": ">= 8.0.0",
    "yarn": ">= 1.7.0",
    "npm": ">= 5.0.0"
  },
  "dependencies": {
    "@marblejs/core": "2.1.0",
    "@marblejs/middleware-body": "2.1.0",
    "@marblejs/middleware-cors": "2.1.0",
    "@marblejs/middleware-io": "2.1.0",
    "@marblejs/middleware-jwt": "2.1.0",
    "@marblejs/middleware-logger": "2.1.0",
    "@types/faker": "^4.1.4",
    "@types/joi": "^13.6.0",
    "@types/mongoose": "^5.2.12",
    "@types/node": "^10.1.2",
    "@types/uuid": "^3.4.4",
    "chalk": "^2.4.1",
    "faker": "^4.1.0",
    "mongoose": "^5.2.14",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.3.3",
    "tsconfig-paths": "^3.7.0",
    "typegoose": "^5.2.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/mock-fs": "^3.6.30",
    "@types/supertest": "^2.0.4",
    "hercule": "^4.1.1",
    "husky": "^0.14.0",
    "jest": "^23.5.0",
    "lerna": "^2.11.0",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.17.3",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "~23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.1"
  }
}
